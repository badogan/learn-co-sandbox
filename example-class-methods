class BasriModifyNumbers
    def initialize(array,some_number_to_check)  
        # Instance variables  
        @array = array  
        @some_number_to_check = some_number_to_check
    end  

    def Multiply
        multiplied=1
        @array.each {|item|
            multiplied *= item
        }
        multiplied
    end

    def Add
        added=0
        @array.each {|item|
            added += item
        }
        added
    end
    
